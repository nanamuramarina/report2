
<h1><strong>Books</strong></h1>
<table>
<thead>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th colspan="3"></th>
  </tr>
</thead>
</table>


<h2><strong>New Book</strong></h2>
<form class="new_book" id=new_book action="/books" accept-cherset="UTF-8" method="post">
<%= form_with model: Book.new do |f| %>
 <h4>Title</h4>
 <%= f.text_field :title %>
 
 <h4>本文</h4>
 <%= f.text_area :body %>
 
 <% f.submit 'Create Book'　%>
 <% end %>
</form>
